{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@wallet-ops-arbitrum/core": ["packages/core/src/index"],
      "@wallet-ops-arbitrum/core/*": ["packages/core/src/*"]
    }
  }
}
